{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "vite.env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/*.vue",
    "src/*.ts",
    "src/*.js",
  ],
  "exclude": [
    "src/**/__tests__/*",
    "备份/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  }
}